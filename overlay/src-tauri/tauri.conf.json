{
	"productName": "MemeOver",
	"version": "0.4.5",
	"identifier": "memeover",
	"build": {
		"frontendDist": "../dist",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "pnpm run dev",
		"beforeBuildCommand": "pnpm run build"
	},
	"app": {
		"windows": [
			{
				"title": "MemeOver",
				"width": 800,
				"height": 600,
				"fullscreen": false,
				"x": 50,
				"y": 50,
				"focus": false,
				"resizable": false,
				"alwaysOnTop": true,
				"transparent": true,
				"decorations": false,
				"hiddenTitle": true,
				"shadow": false
			}
		],
		"security": {
			"csp": null
		},
		"macOSPrivateApi": true
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}

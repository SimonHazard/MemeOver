{
  "name": "memeover",
  "productName": "memeover",
  "version": "0.1.3",
  "description": "An overlay app to integrate to Discord",
  "main": ".vite/build/main.js",
  "repository": "https://github.com/SimonHazard/MemeOver",
  "scripts": {
    "start": "electron-forge start",
    "package:windows": "electron-forge package --platform=win32",
    "package:linux": "electron-forge package --platform=linux",
    "package:macos": "electron-forge package --platform=darwin",
    "package": "electron-forge package",
    "make:windows": "electron-forge make --platform=win32",
    "make:linux": "electron-forge make --platform=linux",
    "make:macos": "electron-forge make --platform=darwin",
    "make": "electron-forge make",
    "publish:windows": "electron-forge publish --platform=win32",
    "publish:linux": "electron-forge publish --platform=linux",
    "publish:macos": "electron-forge publish --platform=darwin",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "30.0.9",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "update-electron-app": "^3.0.0",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "Simon Hazard",
    "email": "simon.hazard@outlook.fr"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
